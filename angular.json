{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm",
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "newProjectRoot": "projects",
  "projects": {
    "app-shared": {
      "projectType": "library",
      "root": "projects/app-shared",
      "sourceRoot": "projects/app-shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/app-shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/app-shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/app-shared/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/app-shared/**/*.ts",
              "projects/app-shared/**/*.html"
            ],
            "eslintConfig": "projects/app-shared/eslint.config.js"
          }
        }
      }
    },
    "handset": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/handset",
      "sourceRoot": "projects/handset/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/handset/src/main.ts",
            "baseHref": "/app/handset/",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "projects/handset/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/handset/public"
              }
            ],
            "styles": [
              "projects/handset/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "handset:build:production"
            },
            "development": {
              "buildTarget": "handset:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/handset/**/*.ts",
              "projects/handset/**/*.html"
            ],
            "eslintConfig": "projects/handset/eslint.config.js"
          }
        }
      }
    },
    "web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/web",
      "sourceRoot": "projects/web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/web/src/main.ts",
            "baseHref": "/app/web/",
            "polyfills": [
              "zone.js",
              "@angular/localize/init",
              "projects/web/src/polyfills.ts"
            ],
            "tsConfig": "projects/web/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/web/public"
              },
              {
                "input": "node_modules/@ant-design/icons-angular/src/inline-svg/",
                "glob": "**/*.svg",
                "output": "assets"
              },
              {
                "input": "node_modules/bootstrap-icons/icons",
                "glob": "*.svg",
                "output": "assets/bi"
              }
            ],
            "styles": [
              {
                "input": "node_modules/ng-zorro-antd/ng-zorro-antd.variable.css",
                "bundleName": "ng-zorro-antd",
                "inject": true
              },
              "projects/web/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "host": "127.0.0.1"
          },
          "configurations": {
            "production": {
              "buildTarget": "web:build:production"
            },
            "development": {
              "buildTarget": "web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/web/**/*.ts",
              "projects/web/**/*.html"
            ],
            "eslintConfig": "projects/web/eslint.config.js"
          }
        }
      }
    }
  }
}
