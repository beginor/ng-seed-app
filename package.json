{
  "name": "ng-seed-cli",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist out-tsc",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web": "ng build web --configuration production",
    "build-handset": "ng build handset --configuration production",
    "watch-web": "ng build web --watch --configuration development",
    "watch-handset": "ng build handset --watch --configuration development",
    "start": "pnpm run watch-web"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@angular/localize": "~13.0.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.1",
    "@popperjs/core": "^2.10.2",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0-beta.4",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@angular/cdk": "~13.0.0",
    "@angular/material": "~13.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.5.2"
  }
}
