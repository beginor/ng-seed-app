{
  "name": "ng-seed-cli",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist out-tsc",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web": "ng build web --configuration production",
    "build-handset": "ng build handset --configuration production",
    "watch-web": "ng build web --watch --configuration development",
    "watch-handset": "ng build handset --watch --configuration development",
    "start": "pnpm run watch-web"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.1",
    "@popperjs/core": "^2.10.2",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0-beta.4",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@angular/cdk": "~13.0.0",
    "@angular/material": "~13.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "^8.57.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.4.2"
  }
}
