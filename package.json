{
  "name": "ng-seed-cli",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist out-tsc",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web": "ng build web --configuration production",
    "build-handset": "ng build handset --configuration production",
    "serve-web": "ng serve web --watch --configuration development",
    "serve-handset": "ng serve handset --watch --configuration development",
    "start": "pnpm run serve-web"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "@angular/localize": "^17.3.0",
    "@angular/cdk": "^17.3.0",
    "ng-zorro-antd": "^17.4.0",
    "@ant-design/icons-angular": "^17.0.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "8.57.0",
    "ng-packagr": "^18.1.0",
    "typescript": "~5.5.2"
  }
}
