{
  "name": "ng-seed-cli",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist out-tsc",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web": "ng build web --configuration production",
    "build-handset": "ng build handset --configuration production",
    "serve-web": "ng serve web --watch --configuration development",
    "serve-handset": "ng serve handset --watch --configuration development",
    "start": "pnpm run serve-web"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "@angular/localize": "^18.1.0",
    "@angular/cdk": "^18.1.0",
    "ng-zorro-antd": "^18.0.1",
    "@ant-design/icons-angular": "^18.0.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.0",
    "@angular/build": "^19.1.0",
    "@angular/cli": "^19.1.0",
    "@angular/compiler-cli": "^19.1.0",
    "angular-eslint": "19.0.2",
    "eslint": "^9.16.0",
    "ng-packagr": "^19.1.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.18.0"
  }
}
